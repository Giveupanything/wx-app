/* pages/visitorRecord/visitorRecord.wxss */
.visitor-title {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    box-sizing: border-box;
    padding-left: 30rpx;
    background-color: #eee;
    font-size: 28rpx;
    color: #666;
}

.tag {
    /* border: 1rpx solid #000; */
    border-radius: 12rpx;
    padding: 6rpx 12rpx;
    font-size: 26rpx;
}

.success {
    background-color: #EBF8F1;
    color: #2FA766;
}

.cancel {
    background-color: rgb(214, 211, 210);
    color: rgb(167, 162, 162);
}